                <apex:column value="{!acc.billingstate}" />
            </apex:pageBlockTable>
            <apex:dataTable value="{!accountList}" var="acc">
                <apex:column value="{!acc.name}"/>
            </apex:dataTable>
        </apex:pageBlock>
    </apex:form>
</apex:page>
